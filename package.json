{
	"name": "whoknows",
	"displayName": "Who Knows",
	"version": "0.1.0",
	"publisher": "rafaelkallis",
	"description": "Who Knows? - a status bar contribution that reports the best person to ask a question for the file being edited.",
	"author": {
		"name": "Rafael Kallis"
	},
	"categories": [
		"Other"
	],
	"license": "SEE LICENSE IN LICENSE.txt",
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
	"activationEvents": [
		"*"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/rafaelkallis/who-knows.git"
	},
	"engines": {
		"vscode": "0.10.x"
	},
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"vscode": "0.10.x",
		"typescript": "^1.6.2"
	},
	"dependencies": {
		"degree-of-authorship": "1.0.0"
	}
